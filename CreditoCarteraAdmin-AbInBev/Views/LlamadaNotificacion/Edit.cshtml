@model CreditoCarteraAdmin_AbInBev.Models.LlamadaNotificacion

@{
    ViewData["Title"] = "Edit";
}

<h1 class="text-white-50">Edit</h1>

<h4 class="text-white">LlamadaNotificacion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="BlobStorageURL" class="control-label"></label>
                <input asp-for="BlobStorageURL" class="form-control" />
                <span asp-validation-for="BlobStorageURL" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DiasPrevios" class="control-label"></label>
                <input asp-for="DiasPrevios" class="form-control" />
                <span asp-validation-for="DiasPrevios" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
